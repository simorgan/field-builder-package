{
    "name": "simonmorgan/field-builder",
    "description": "Field UI components for Laravel applications with Livewire support",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Simon Morgan",
            "email": "simonmorgan1000@hotmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "livewire/livewire": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Simonmorgan\\FieldBuilder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Simonmorgan\\FieldBuilder\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Simonmorgan\\FieldBuilder\\FieldBuilderServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
